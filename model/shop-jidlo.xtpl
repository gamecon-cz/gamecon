<!-- begin:jidlo -->
<!-- begin:potize -->
<p>Objednat nebo změnit objednávky jídla dočasně není možné. Zkus to prosím později.</p>
<!-- end:potize -->
<!-- begin:pozastaveno -->
<p>Objednat nebo změnit objednávky jídla již není možné.</p>
<!-- end:pozastaveno -->
<table class="shopJidlo_tabulka">
  <tr>
    <td></td>
    <!-- begin:den -->
    <td><div class="shop_den">{den}</div></td>
    <!-- end:den -->
  </tr>
  <!-- begin:druh -->
  <tr>
    <td>
      <div class="shop_popis shopJidlo_popis">
        {druh}
        <div class="shop_popisCena">{cena}</div>
      </div>
    </td>
    <!-- begin:den -->
    <td>
      <!-- begin:checkbox -->
      <input type="checkbox" class="shopJidlo_checkbox" name="{pnName}" {selected}>
      <!-- end:checkbox -->
      <!-- begin:locked -->
      <input type="hidden" name="{pnName}" value="{selected}">
      <input type="checkbox" class="shopJidlo_checkbox" {selected} disabled>
      <!-- end:locked -->
    </td>
    <!-- end:den -->
  </tr>
  <!-- end:druh -->
</table>
<input type="hidden" name="{pnJidloZmen}" value="1">
<!-- end:jidlo -->
