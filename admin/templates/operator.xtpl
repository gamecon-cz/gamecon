<!-- begin:operator -->
<div class="operator">
  <label for="omniboxUzivatel">
    <b>Přihlášen{a} jako</b><br>
    {operator}
  </label>
  <!-- begin:prepnutiUzivatele -->
  <form method="post">
    <input type="hidden" name="prihlasitSeJakoUzivatel" value="1">
    <span class="hinted">
      <input id="omniboxUzivatel" class="omnibox autosubmit" type="text" name="id" style="max-width: 95%">
      <span class="hint" style="max-width: 85%">
        ↑ vyber uživatele: napiš pár písmen přezdívky, jména, příjmení nebo ID
      </span>
    </span>
  </form>
  <!-- end:prepnutiUzivatele -->
  <form method="post">
    <input type="submit" name="odhlasNAdm" value="Odhlásit"/>
  </form>
</div>
<!-- end:operator -->
